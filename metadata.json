{
  "name": "camptocamp-deploy",
  "version": "1.0.10",
  "author": "Camptocamp",
  "summary": "Puppet Deploy module",
  "license": "Apache-2.0",
  "source": "https://github.com/camptocamp/puppet-deploy",
  "project_page": "https://github.com/camptocamp/puppet-deploy",
  "issues_url": "https://github.com/camptocamp/puppet-deploy/issues",
  "dependencies": [
    {
      "name": "herculesteam/augeasproviders_core",
      "version_requirement": "2.x"
    },
    {
      "name": "herculesteam/augeasproviders_ssh",
      "version_requirement": "2.x"
    },

    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.2.0 <5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
